<Window x:Class="DrNuDownloader.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:view="clr-namespace:DrNuDownloader.Wpf.View"
        xmlns:viewModel="clr-namespace:DrNuDownloader.Wpf.ViewModel"
        Title="DR NU Downloader" Height="115" Width="525" ResizeMode="NoResize">
    <Window.Resources>
        <ResourceDictionary Source="ResourceDictionary.xaml" />
    </Window.Resources>
    <Grid>
        <view:DownloadView Content="{Binding DownloadViewModel}" />
    </Grid>
</Window>